<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="double_slash.techtown.com.Phoneosk">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Phoneosk"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="double_slash.techtown.com.phoneosk.MenuCartActivity"/>
        <activity android:name="double_slash.techtown.com.phoneosk.FinalActivity"/>
        <activity android:name="double_slash.techtown.com.phoneosk.MenuActivity"/>
        <activity android:name="double_slash.techtown.com.phoneosk.QR_Activity"/>

        <activity android:name="double_slash.techtown.com.phoneosk.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="fullSensor"
            tools:replace="screenOrientation" />

    </application>


</manifest>